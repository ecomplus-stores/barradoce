<%
for (let i = 0; i < _.widgets.length; i++) {
  const { pkg, options } = _.widgets[i]
  const partialSrc = _.widgets[i][field]
  if (partialSrc) {
    const _options = _.pageName !== 'app/' ? options : {
      ...options,
      partytown: false
    }
    try {
      %> <%- await include(`${pkg}/${partialSrc}`, { _, opt, options: _options }) %> <%
    } catch (err) {
      console.error(err)
    }
  }
}
%>
